    "def to_big(str):\n",
    "    list = str.split(\",\")\n",
    "    new = []\n",
    "    txt = \"\"\n",
    "    for i in list:\n",
    "        z = i.upper()\n",
    "        new.append(z)\n",
    "    txt = (\",\").join(new)\n",
    "    return txt\n"
   ]
